{
  "name": "openapi-zod-ts-fe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "gen:types": "openapi-typescript ./openapi-zod-ts-schema/api-schema.yaml -o ./src/types/api-schema.d.ts --root-types && node ./scripts/exportToDeclare.js ./src/types/api-schema.d.ts",
    "gen:zod": "typed-openapi ./openapi-zod-ts-schema/api-schema.yaml -o src/types/api-schema.zod.d.ts -r zod"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "openapi-typescript": "^7.6.1",
    "typed-openapi": "^0.10.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "zod": "^3.24.2"
  }
}
